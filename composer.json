{
	"name": "bloody-hell/yii2-rating-widget",
	"description": "Star rating widget for Yii2.",
	"keywords": ["yii", "extension", "widget", "rating"],
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Ivan Poponin",
			"email": "juf.halli@gmail.com"
		}
	],
    "repositories": [
        {
            "type":"package",
            "package":{
                "name":"fyneworks/jquery-star-rating",
                "version":"4.11",
                "source":{
                    "type":"git",
                    "url":"https://github.com/Fyneworks-jQuery/star-rating",
                    "reference":"master"
                }
            }
        }
    ],
	"minimum-stability": "dev",
	"require": {
		"yiisoft/yii2": "master",
        "fyneworks/jquery-star-rating": "*"
	},
	"autoload": {
		"psr-4": {
			"bloody_hell\\rating\\": ""
		}
	}
}
